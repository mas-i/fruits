= render 'devise/shared/flash_messages'
- @fruits.each do |fruit|
  #carouselExampleIndicators.carousel.slide{"data-ride" => "carousel"}
    %ol.carousel-indicators
      %li.active{"data-slide-to" => "0", "data-target" => "#carouselExampleIndicators"}
      %li{"data-slide-to" => "1", "data-target" => "#carouselExampleIndicators"}
      %li{"data-slide-to" => "2", "data-target" => "#carouselExampleIndicators"}
    .carousel-inner
      .carousel-item.active
        %svg.bd-placeholder-img.bd-placeholder-img-lg.d-block.w-100{"aria-label" => "Placeholder: First slide", :focusable => "false", :height => "300", :preserveaspectratio => "xMidYMid slice", :role => "img", :width => "800", :xmlns => "http://www.w3.org/2000/svg"}
          %title Placeholder
          %rect{:fill => "#777", :height => "100%", :width => "100%"}
          %text{:dy => ".3em", :fill => "#555", :x => "50%", :y => "50%"} First slide
      .carousel-item
        %svg.bd-placeholder-img.bd-placeholder-img-lg.d-block.w-100{"aria-label" => "Placeholder: Second slide", :focusable => "false", :height => "300", :preserveaspectratio => "xMidYMid slice", :role => "img", :width => "800", :xmlns => "http://www.w3.org/2000/svg"}
          %title Placeholder
          %rect{:fill => "#666", :height => "100%", :width => "100%"}
          %text{:dy => ".3em", :fill => "#444", :x => "50%", :y => "50%"} Second slide
      .carousel-item
        %svg.bd-placeholder-img.bd-placeholder-img-lg.d-block.w-100{"aria-label" => "Placeholder: Third slide", :focusable => "false", :height => "300", :preserveaspectratio => "xMidYMid slice", :role => "img", :width => "800", :xmlns => "http://www.w3.org/2000/svg"}
          %title Placeholder
          %rect{:fill => "#555", :height => "100%", :width => "100%"}
          %text{:dy => ".3em", :fill => "#333", :x => "50%", :y => "50%"} Third slide
    %a.carousel-control-prev{"data-slide" => "prev", :href => "#carouselExampleIndicators", :role => "button"}
      %span.carousel-control-prev-icon{"aria-hidden" => "true"}
      %span.sr-only Previous
    %a.carousel-control-next{"data-slide" => "next", :href => "#carouselExampleIndicators", :role => "button"}
      %span.carousel-control-next-icon{"aria-hidden" => "true"}
      %span.sr-only Next

